PUSHN 4
START
	PUSHS "Escreva o número de inteiros que pretende escrever.\n"
	WRITES
	READ
	ATOI
	STOREG 1
	PUSHS "Insira os "
	WRITES
	PUSHG 1
	WRITEI
	PUSHS " números:\n"
	WRITES
	PUSHG 1
	PUSHI 1
SUB
	STOREG 1
	READ
	ATOI
	STOREG 2
	PUSHG 1
	STOREG 0
repeat1:
	PUSHG 0
	PUSHI 0
	SUP
	JZ repeatEnd2
	READ
	ATOI
	STOREG 3
	PUSHG 3
	PUSHG 2
	INF
	JZ if3
	PUSHG 3
	STOREG 2
if3:
	PUSHG 0
	PUSHI 1
	SUB
	STOREG 0
	JUMP repeat1
repeatEnd2:
	PUSHS "Menor número: "
	WRITES
	PUSHG 2
	WRITEI
	PUSHS "\n"
	WRITES
STOP